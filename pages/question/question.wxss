.hd {
	
	width: 100%;
}
.hd > .label {
	height: 70rpx;
	line-height: 70rpx;
	min-width: 196rpx;
	box-sizing: border-box;
	padding: 16rpx 50rpx 16rpx 32rpx;
	border-radius: 0 40rpx 40rpx 0;
	background-color: #4674de;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
}
.hd > .title {
	font-size: 36rpx;
	color: #fff;
	padding: 0 78rpx;
	text-align: center;
	line-height: 50rpx;
	margin-bottom:100rpx;
}
.hd > .time {
	color: #b9e84c;
	font-weight: 900;
	font-size: 129rpx;
	text-align: center;
	width: 100%;
	height: 200rpx;
	line-height: 200rpx;
}
.hd .time  .time-small {
	font-size: 80rpx!important;
	font-weight: bold!important;
}
.hd > .time-danger {
	color: #f3594b!important;
}
.question-list {
	width: 100%;
}
.question-list > .item {
	width: 604rpx;
	height: 105rpx;
	margin: 0 auto 30rpx;
	border-radius: 18rpx;
	font-size: 30rpx;
	box-shadow: 0rpx 6rpx 0rpx 0.08rpx rgba(70, 135, 217, 0.6);
	position: relative;
} 
.question-list > .item view {
	width: 100%;
	padding: 0 72rpx;
	box-sizing: border-box;
	text-align: center;
	font-size: 30rpx;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
}
.item-default {
	color: #666;
	background-color: #fff;
}
.item-selected {
	color: #fff;
	background-color: #999;
}
.item-right, .item-right-more {
	color: #fff;
	background-color: #a4d239;
	position: relative;
}
.item-wrong {
	color: #fff;
	background-color: #f3594b;
	position: relative;
}
.item-right::before, .item-wrong::before {
	left: 36rpx;
}
.item-right::after, .item-wrong::after {
	right: 36rpx;
}
.item-right::after, .item-right::before {
	background-image: url("data:image/png;base64,");
}
.item-right::after, .item-right::before, .item-wrong::after, .item-wrong::before {
	content: "";
	display: block;
	background-size: cover;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	width: 26rpx;
	height: 26rpx;
}
.item-wrong::after, .item-wrong::before {
	background-image: url("data:image/png;base64,");
}
.ok-btn {
	margin: 72rpx auto 0;
}
.box-border {
	background-color: rgba(63, 140, 244, 0.3);
}
.box-border {
	width: 636rpx;
	height: 790rpx;
}
.box-bg {	
	width: 600rpx;
	height: 750rpx;
}
.box-name {	
	height: 108rpx;
	line-height: 108rpx;
}
.i-gold {
	width: 303rpx;
	height: 187rpx;
	display: block;
	margin: 120rpx auto 40rpx;
}
.popup .content {
	width: 100%;
	font-size: 36rpx;
	text-align: center;
	line-height: 56rpx;
}
.popup .tip {
	width: 100%;
	font-size: 30rpx;
	color: #999;
	text-align: center;
}
.like {
  color: #fff;
  position: absolute;
  top: 140rpx;
  left: 554rpx;
}
.like > image {
  width: 50rpx;
  height: 52rpx;
  margin-right: 18rpx;
}
.like > text {
  font-size: 24rpx;
}
.like > .txt-liked {
  color: #fff100;
}
.like > view {
  font-size: 20rpx;
}

